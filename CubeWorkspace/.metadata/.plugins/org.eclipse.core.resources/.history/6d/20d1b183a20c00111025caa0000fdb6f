#include "algo.h"
#include <stm32l476xx.h>

void UTILS_WaitN10ms(int N){
	int n,i,s=0;
	for(n=1;n<=N;n++){
		for(i=0;i<100000;i++){
			s=s+i;
		}
	}
}


void setup(){
	TIM2->CR1 |= 1;

}
